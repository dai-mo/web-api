{
  "scripts": {
    "start": "tsc -p ./test && concurrently \"npm run tsc:w -- -p ./test\" \"npm run lite\" ",
    "lite": "lite-server",
    "pretest": "rm -rf ./test/dist && npm run tsc -- -p ./test",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "lite-server-test": "lite-server --config=./test/liteserver-test-config.json",
    "test": "karma start karma.conf.js",
    "posttest": "node_modules/.bin/remap-istanbul -i test/coverage/coverage-final.json -o test/coverage -t html",
    "coverage": "http-server -c-1 -o -p 9875 ./test/coverage"
  },
